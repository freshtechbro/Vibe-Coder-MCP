{
  "name": "nuxt-trpc",
  "version": "1.0.0",
  "description": "Full-stack starter kit with Nuxt and tRPC",
  "metadata": {
    "framework": "Nuxt",
    "language": "TypeScript",
    "styling": "Tailwind CSS",
    "database": "PostgreSQL",
    "orm": "Prisma",
    "features": {
      "authentication": {
        "jwt": true,
        "oauth": ["google", "github"],
        "session": true,
        "rbac": true
      },
      "database": {
        "migrations": true,
        "seeding": true,
        "typeGeneration": true
      },
      "deployment": {
        "docker": true,
        "ci": true
      }
    }
  }
}
